{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family" : "LearnJenkinsApp",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "#ECR_URI#:#APP_VERSION#",
            "portMapping": [{
                "name": "nginx-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::654652481644:role/ecsTaskExecutionRole"
}